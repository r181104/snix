super + Return
	alacritty

super + shift + Return
	kitty

super + b
	brave

super + e
	thunar

super + @space
	rofi -show drun

control + @space
    ~/snix/scripts/wset

super + Escape
	pkill -USR1 -x sxhkd

super + alt + l
	i3lock --blur=5

super + alt + {q, r}
	bspc {quit, wm -r}

super + {_,shift + }q
	bspc node -{c, k}

super + shift + t
	bspc desktop -l next

super + {_,shift + }{1-9,0}
    bspc {desktop -f,node -d} ^{1-9,10} --follow

super + {j,k}
    bspc desktop -f {prev,next}.local

super + shift + Tab
    bspc node -d last.occupied --follow

super + f
    bspc node -t ~fullscreen

alt + {h,j,k,l}
    bspc node -f {west,south,north,east}

alt + shift + {h,j,k,l}
     bspc node -s {west,south,north,east}
